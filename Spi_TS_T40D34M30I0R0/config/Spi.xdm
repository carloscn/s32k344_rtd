          <!-- @implements SPI_Object -->
          <d:chc name="Spi" type="AR-ELEMENT" value="MODULE-DEF">
            <v:ctr type="MODULE-DEF">
              <a:a name="RELEASE" value="asc:4.7"/>
              <a:a name="ADMIN-DATA" type="ADMIN-DATA">
                 <ad:ADMIN-DATA>
                   <ad:DOC-REVISIONS>
                     <ad:DOC-REVISION>
                       <ad:REVISION-LABEL>3.0.0</ad:REVISION-LABEL>
