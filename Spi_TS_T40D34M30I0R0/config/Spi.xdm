                           <a:tst expr="contains(../SpiPhyUnitMapping, 'LPSPI') and node:refvalid(.)" true="This node must be disabled when HWUnit is LPSPI"/>
                        </a:da>
                     </v:ref>
                     
                  </v:ctr>
                </v:lst>
              </v:ctr>
