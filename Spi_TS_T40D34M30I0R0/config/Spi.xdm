                      </a:a>
                      <a:a name="UUID" value="ECUC:d87c39ab-c597-4b98-adff-4260f28fe9b5"/>
                      <!-- @implements SpiInterruptibleSequence_Object -->
                      <v:var name="SpiInterruptibleSequence" type="BOOLEAN">
                        <a:a name="DESC">
