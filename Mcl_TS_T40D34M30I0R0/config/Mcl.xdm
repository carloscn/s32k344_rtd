                                <a:da name="INVALID" type="XPath">
                                   <a:tst expr="(../../MclGeneral/MclDma/MclEnableDma ='true') and (num:i(count(./*)) > num:i(count(ecu:list('Mcl.MclConfigSet.DmaHwInstance.List'))))"
                                   true="Number of the Dma Instances configured is greater than number of the existing Dma hardware Instances on the platform."/>
                                   <a:tst expr="(../../MclGeneral/MclDma/MclEnableDma ='true') and (num:i(count(./*))=0)"
