                                    <a:da name="DEFAULT" value="true"/>
                                  </v:var>
              <!-- /* @implements UartCallbackCapability_Object */ -->
                                <v:var name="UartCallbackCapability" type="BOOLEAN">
                                    <a:a name="DESC">
                                        <a:v><![CDATA[EN:<html><p> When this parameter is enabled, Callback feature will be used in Uart driver, with the following functions:
                                         a) completion of a reception or sending operation.
                                         b) reporting communication errors.
                                         </p></html>]]></a:v>
                                    </a:a>
