                                    <a:a name="IMPLEMENTATIONCONFIGCLASS"
                                         type="IMPLEMENTATIONCONFIGCLASS">
                                      <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                      <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="NXP"/>
                                    <a:a name="SCOPE" value="LOCAL"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="true"/>
                                    <a:a name="UUID" value="ECUC:aac902ef-7cb7-4b94-acc5-b6c4ca835e7a"/>
                                    <a:da name="DEFAULT" type="XPath" expr="node:fallback(node:current()/../@index, num:i(1))"/>
                                    <a:da name="INVALID" type="Range">
                                        <a:tst expr="&lt;=ecu:get('Uart.UartGlobalConfig.UartChannel')"/>
                                        <a:tst expr="&gt;=0"/>
                                    </a:da>
                                    <a:da name="RANGE" type="XPath">
                                        <a:tst expr="(. &gt;= 0) and (.&lt; num:i(count(node:current()/../../*)))"
                                               false="Value out of range: must be in range 0 to N-1 (N is number of configured channels). Use the 'Calculate Value' button to calculate correct default value."/>
                                        <a:tst expr="text:uniq(../../*/UartChannelId, .)"
                                               false="Duplicated value: UartChannelId must be unique across all Uart channels. Use the 'Calculate Value' button to calculate correct default value."/>
                                        <a:tst expr="num:i(node:fallback(.,0)) = num:i(../@index)"
                                               false="Invalid value: The ID should correspond to the position of the element within the configuration list."/>
                                    </a:da>
                                  </v:var>


                  <!-- /* @implements UartClockRef_Object */ -->
                                    <v:ref name="UartClockRef" type="REFERENCE">
                                        <a:a name="DESC">
                                        <a:v><![CDATA[EN:
                                                <html>
                                                Reference to the Uart clock source configuration, which is set into the MCU driver configuration.
                                                This clock source is used for configure Uart baudrate.</html>
                                            ]]>
                                        </a:v>
                                        </a:a>
                                        <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                            <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                            <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                        </a:a>
                                        <a:a name="ORIGIN" value="NXP"/>
                                        <a:a name="SCOPE" value="LOCAL"/>
                                        <a:a name="UUID" value="ECUC:ab3c291e-8312-49d9-9032-2edf0ed55ee0"/>
                                        <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                        <a:da name="REF"
                                                value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/Mcu/McuModuleConfiguration/McuClockSettingConfig/McuClockReferencePoint"/>

                                                    <a:da name="INVALID" type="XPath">
                                                    <a:tst expr="node:refvalid(.)" false="Invalid or empty reference."/>
                                        </a:da>
                                    </v:ref>

                                    <v:ref name="UartChannelEcucPartitionRef" type="REFERENCE">
                                        <a:a name="DESC">
                                            <a:v>
                                                <![CDATA[EN:<html>
                                                    Maps one single Uart channel to zero or one ECUC partitions.
                                                    The ECUC partition referenced is a subset of the ECUC partitions where the Uart driver is mapped to.
                                                </html>]]>
                                            </a:v>
                                        </a:a>
                                        <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                          <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                          <icc:v class="PreCompile">VariantPostBuild</icc:v>
                                        </a:a>
                                        <a:a name="OPTIONAL" value="true"/>
                                        <a:a name="ORIGIN" value="NXP"/>
                                        <a:a name="SCOPE" value="ECU"/>
                                        <a:a name="UUID" value="ECUC:b016c50a-d2c6-4fe5-b096-7569bfac79ff"/>
                                        <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                        <a:a name="POSTBUILDVARIANTMULTIPLICITY" value="true"/>
                                        <a:da name="REF" value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/EcuC/EcucPartitionCollection/EcucPartition"/>
                                        <a:a name="EDITABLE" type="XPath">
                                            <a:tst expr="../../../../GeneralConfiguration/UartMulticoreSupport ='true'"/>
                                        </a:a>
                                        <a:da name="INVALID" type="XPath">
                                            <a:tst expr="not(node:refvalid(.)) and (../../../../GeneralConfiguration/UartMulticoreSupport ='true')" true="When UartMulticoreSupport enabled, The ECUC Partition for UartChannels must be configured."/>
                                            <a:tst expr="not(node:containsValue(../../../../GeneralConfiguration/UartEcucPartitionRef/*, node:value(.))) and (../../../../GeneralConfiguration/UartMulticoreSupport ='true') and (node:refvalid(.))"
                                                true="The ECUC partitions referenced by UartChannelEcucPartitionRef shall be a subset of the ECUC partitions referenced by UartEcucPartitionRef."/>
                                        </a:da>
                                    </v:ref>
                  <!-- /* @implements DetailModuleConfiguration_Object */ -->
                                  <v:ctr name="DetailModuleConfiguration" type="IDENTIFIABLE">
                                      <a:a name="ENABLE" type="XPath">
                                          <a:tst expr="contains(., substring-after(string(node:fallback(../UartHwUsing,'FLEXIO_IP')),'FLEXIO'))"/>
                                      </a:a>
                                      <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:
                                                <html>
                                                This container contains the configuration (parameters) of the Module Configuration.
                                                </html>
                                            ]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="POSTBUILDVARIANTMULTIPLICITY" value="false"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS"
                                        type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v mclass="PreCompile">VariantPostBuild</icc:v>
                                        <icc:v mclass="PreCompile">VariantPreCompile</icc:v>
                                    </a:a>
                                    <a:a name="UUID" value="ECUC:fdac1ad9-1319-44a8-bc4f-341635a10162"/>
                                    <a:a name="LABEL" value="Module configuration"/>

                                    <v:var name="UartHwChannel" type="ENUMERATION">
                                      <a:a name="DESC">
                                          <a:v>
                                              <![CDATA[EN:
                                                  <html>
                                                  Selects the physical Lpuart Uart Channel. <p/>
                                                  Note: Implementation Specific Parameter. <p/>
                                                  </html>
                                              ]]>
                                          </a:v>
                                      </a:a>
                                      <a:a name="LABEL" value="Uart hardware channel" />
                                      <a:a name="IMPLEMENTATIONCONFIGCLASS"
                                           type="IMPLEMENTATIONCONFIGCLASS">
                                          <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                          <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                      </a:a>
                                      <a:a name="ORIGIN" value="NXP"/>
                                      <a:a name="SCOPE" value="LOCAL"/>
                                      <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                      <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                      <a:a name="UUID" value="ECUC:38348b20-3249-031a-ab13-8afb524661cd"/>
                                      <a:da name="DEFAULT" type="XPath" expr="(ecu:list('Uart.UartGlobalConfig.UartChannel.LpuartHWChannelList'))[position()-1=node:fallback( node:current()/../../@index, '0')]">
                                      </a:da>
                                       <a:da name="INVALID" type="XPath">
                                        <a:tst expr="text:uniq(node:fallback(../../../*/DetailModuleConfiguration/UartHwChannel, 0 ), node:fallback( ., 1 ))"
                                               false="Duplicate physical Uart channel: UartHwChannel must be unique across all Uart channels. Use the 'Calculate Value' button to calculate correct default value."/>
                                        <a:tst expr="contains(node:fallback(node:refs('ASPathDataOfSchema:/AUTOSAR/EcucDefs/Lin')/LinGlobalConfig/LinChannel/*/LinHwChannel, 0 ), node:fallback(concat('LPUART_IP_', substring-after(string(.), 'LPUART_')), 1 ))"
                                               true="Duplicate physical Uart channel: UartHwChannel is being used by other drivers. It must be unique across all drivers."/>
                                      </a:da>
                                      <a:da name="RANGE" type="XPath"
                                            expr="ecu:list('Uart.UartGlobalConfig.UartChannel.LpuartHWChannelList')"/>
                                    </v:var>

                  <!-- /* @implements DesireBaudrate_Object */ -->
                                    <v:var name="DesireBaudrate" type="ENUMERATION">
                                      <a:a name="DESC">
                                        <a:v><![CDATA[EN:
                                          <html>
                                            This parameter is user's desire baudrate. <p/>
                                            Note: Implementation Specific Parameter. <p/>
                                          </html>
                                                ]]>
                                        </a:v>
                                      </a:a>
                                      <a:a name="IMPLEMENTATIONCONFIGCLASS"
                                           type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                      </a:a>
                                      <a:a name="ORIGIN" value="NXP"/>
                                      <a:a name="SCOPE" value="LOCAL"/>
                                      <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                      <a:a name="UUID" value="ECUC:0b8162ce-d384-46d5-952a-1af3295ca75d"/>
                                      <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                      <a:da name="DEFAULT" value="LPUART_UART_BAUDRATE_9600"/>
                                      <a:da name="RANGE">
                                          <a:v>LPUART_UART_BAUDRATE_CUSTOM</a:v>
                                          <a:v>LPUART_UART_BAUDRATE_1200</a:v>
                                          <a:v>LPUART_UART_BAUDRATE_2400</a:v>
                                          <a:v>LPUART_UART_BAUDRATE_4800</a:v>
                                          <a:v>LPUART_UART_BAUDRATE_7200</a:v>
                                          <a:v>LPUART_UART_BAUDRATE_9600</a:v>
                                          <a:v>LPUART_UART_BAUDRATE_14400</a:v>
                                          <a:v>LPUART_UART_BAUDRATE_19200</a:v>
                                          <a:v>LPUART_UART_BAUDRATE_28800</a:v>
