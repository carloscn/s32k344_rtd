                                  <!-- /* @implements UartMulticoreSupport_Object */ -->
                                  <v:var name="UartMulticoreSupport" type="BOOLEAN">
                                      <a:a name="LABEL" value="Uart Multicore Support"/>
                                      <a:a name="DESC">
                                          <a:v>
                                              <![CDATA[EN:
                                                  <html>
                                                      <h1>UartMulticoreEnable</h1>
                                                      <p>When this parameter is enabled, multi-core feature will be used in Uart driver.
                                                      That means mapping the Uart driver to multiple ECUC partitions to make the module API available in this partition.</p>
                                                      <p>The Uart driver will operate as an independent instance in each of the partitions.</p>
                                                  </html>
                                              ]]>
                                          </a:v>
                                      </a:a>
