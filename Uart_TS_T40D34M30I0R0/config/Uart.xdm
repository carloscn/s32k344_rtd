                                          <a:v>LPUART_UART_BAUDRATE_57600</a:v>
                                          <a:v>LPUART_UART_BAUDRATE_115200</a:v>
                                          <a:v>LPUART_UART_BAUDRATE_230400</a:v>
                                          <a:v>LPUART_UART_BAUDRATE_460800</a:v>
                                          <a:v>LPUART_UART_BAUDRATE_921600</a:v>
                                          <a:v>LPUART_UART_BAUDRATE_1843200</a:v>
                                      </a:da>
                                    </v:var>
                  <!-- /* @implements CustomBaudrateMantissa_Object */ -->
                                <v:var name="CustomBaudrateMantissa" type="INTEGER">
                                    <a:a name="LABEL" value="Custom Baudrate Mantissa (SBR)"/>
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:
                                                <html>
                                                  Identifies the custom baudrate value. <p/>
                                                  Note: Implementation Specific Parameter. <p/>
                                                </html>
                                            ]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="NXP"/>
                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                    <a:a name="SCOPE" value="LOCAL"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:aac902ef-7cb9-2392-acc9-b6c4ca835e15"/>
                                    <a:a name="EDITABLE" type="XPath">
                                      <a:tst expr="(node:fallback(../DesireBaudrate, 'false') = 'LPUART_UART_BAUDRATE_CUSTOM')"/>
                                    </a:a>
                                    <a:da name="DEFAULT" value="1"/>
                                    <a:da name="INVALID" type="Range">
                                        <a:tst expr="&lt;=8191"/>
                                        <a:tst expr="&gt;=1"/>
                                    </a:da>
                                </v:var>
                  <!-- /* @implements CustomBaudrateDivisor_Object */ -->
                                <v:var name="CustomBaudrateDivisor" type="INTEGER">
                                    <a:a name="LABEL" value="Custom Baudrate Divisor (OSR)"/>
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:
                                                <html>
                                                  Identifies the custom baudrate value. <p/>
                                                  Note: Implementation Specific Parameter. <p/>
                                                </html>
                                            ]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="NXP"/>
                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                    <a:a name="SCOPE" value="LOCAL"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:aac902ef-7cb7-239a-acc1-b6c4ca835e1a"/>
                                    <a:a name="EDITABLE" type="XPath">
                                      <a:tst expr="(node:fallback(../DesireBaudrate, 'false') = 'LPUART_UART_BAUDRATE_CUSTOM')"/>
                                    </a:a>
                                    <a:da name="DEFAULT" value="4"/>
                                    <a:da name="INVALID" type="Range">
                                        <a:tst expr="&lt;=32"/>
                                        <a:tst expr="&gt;=4"/>
                                    </a:da>
                                </v:var>
                  <!-- /* @implements CustomBaudrateValue_Object */ -->
                                <v:var name="CustomBaudrateValue" type="INTEGER">
                                    <a:a name="LABEL" value="Custom Baudrate Value"/>
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:
                                                <html>
                                                  Identifies the custom baudrate value. <p/>
                                                  Note: Implementation Specific Parameter. <p/>
                                                </html>
                                            ]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="NXP"/>
                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                    <a:a name="SCOPE" value="LOCAL"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:b490f459-c468-49b8-9dc1-120c2da34d45"/>
                                    <a:a name="EDITABLE" type="XPath">
                                      <a:tst expr="(node:fallback(../DesireBaudrate, 'false') = 'LPUART_UART_BAUDRATE_CUSTOM')"/>
                                    </a:a>
                                    <a:da name="DEFAULT" type="XPath">
                                        <a:tst expr="node:fallback('->num:i(num:i(node:ref(../../UartClockRef)/McuClockReferencePointFrequency) div (num:i(node:fallback(../CustomBaudrateMantissa, 0 )) * num:i(node:fallback(../CustomBaudrateDivisor, 0 ))))', num:i(0))"/>
                                    </a:da>
                                    <a:da name="INVALID" type="XPath">
                                      <a:tst expr="(((node:fallback(., 'false') != num:i(num:i(node:ref(../../UartClockRef)/McuClockReferencePointFrequency) div (num:i(node:fallback(../CustomBaudrateMantissa, 0 )) * num:i(node:fallback(../CustomBaudrateDivisor, 0 ))))))) and (node:fallback(../DesireBaudrate, 'false') = 'LPUART_UART_BAUDRATE_CUSTOM')"
                                          true="is out of range. Please press the button to automatically calculate the correct value"/>
                                    </a:da>
                                </v:var>
                  <!-- /* @implements UartInteruptDmaMethod_Object */ -->
                                    <v:var name="UartInteruptDmaMethod" type="ENUMERATION">
                                      <a:a name="DESC">
                                       <a:v>
                                           <![CDATA[EN:
                                               <html>
                                                 <p>Configures the mechanism used by the 'AsyncSend' or 'AsyncReceive' function (interrupts or DMA).</p>
                                                 Note: Implementation Specific Parameter. <p/>
                                               </html>
                                          ]]>
                                       </a:v>
                                      </a:a>
                                      <a:a name="LABEL" value="Uart Asynchronous Method" />
                                      <a:a name="IMPLEMENTATIONCONFIGCLASS"
                                           type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                      </a:a>
                                      <a:a name="ORIGIN" value="NXP"/>

                                      <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                      <a:a name="SCOPE" value="LOCAL"/>
                                      <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                      <a:a name="UUID" value="ECUC:240b6eec-0e7a-4030-9593-5cc2db9bec41"/>
                                      <a:da name="DEFAULT" value="LPUART_UART_IP_USING_INTERRUPTS"/>
                                      <a:da name="RANGE">
                                          <a:v>LPUART_UART_IP_USING_INTERRUPTS</a:v>
                                          <a:v>LPUART_UART_IP_USING_DMA</a:v>
                                      </a:da>
                                      <a:da name="INVALID" type="XPath">
                                        <a:tst expr="(node:fallback(../UartInteruptDmaMethod, 'LPUART_UART_IP_USING_INTERRUPTS') = 'LPUART_UART_IP_USING_DMA') and
                                        (node:fallback(../../../../../GeneralConfiguration/UartDmaEnable,'false') = 'false')"
                                            true="The UartDmaEnable checkbox in GeneralConfiguration must be checked in order to use DMA as an asynchronous method."/>
                                        <a:tst expr="(
                                                        (node:fallback(.,'LPUART_UART_IP_USING_INTERRUPTS') = 'LPUART_UART_IP_USING_DMA') and
                                                        (not(node:exists(../UartDmaTxChannelRef))) and
                                                        (not(node:exists(../UartDmaRxChannelRef)))
                                                     )"
                                          true="At least one DMA transmit or receive channel reference must be configured. Refer to DetailModuleConfiguration/UartDmaTxChannelRef or DetailModuleConfiguration/UartDmaRxChannelRef."/>
                                      </a:da>
                                    </v:var>

                                    <v:ref name="UartDmaTxChannelRef" type="REFERENCE">
                                      <a:a name="DESC">
                                          <a:v>
                                            <![CDATA[EN:
                                             <html>
                                                <p>Reference to the DMA TX channel, which is set in the Mcl driver configuration.</p>
                                                Note: Implementation Specific Parameter. <p/>
                                            </html>
                                            ]]>
                                          </a:v>
                                      </a:a>
                                      <a:a name="IMPLEMENTATIONCONFIGCLASS"
                                           type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                      </a:a>
                                      <a:a name="OPTIONAL" value="true"/>
                                      <a:a name="SCOPE" value="LOCAL"/>
                                      <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                      <a:a name="LABEL" value="Uart TX DMA Channel"/>
                                      <a:a name="ORIGIN" value="NXP"/>
                                      <a:a name="UUID" value="ECUC:5419eaab-b73b-4ee9-9e88-22e201491b69"/>
                                      <a:a name="EDITABLE" type="XPath">
                                        <a:tst expr="(node:fallback(../UartInteruptDmaMethod, 'LPUART_UART_IP_USING_INTERRUPTS') = 'LPUART_UART_IP_USING_DMA')"/>
                                      </a:a>
                                      <a:da name="INVALID" type="XPath">
                                        <a:tst expr="node:empty(.) and (node:fallback(../UartInteruptDmaMethod, 'LPUART_UART_IP_USING_INTERRUPTS') = 'LPUART_UART_IP_USING_DMA')"
                                               true="Field is empty (Suggestion: select a DMAChannel reference)."/>
                                        <a:tst expr="not(node:refvalid(.)) and (node:fallback(../UartInteruptDmaMethod, 'LPUART_UART_IP_USING_INTERRUPTS') = 'LPUART_UART_IP_USING_DMA')"
                                               true="Invalid DMAChannel reference (Suggestion: select a DMAChannel reference)."/>
                                      </a:da>
                                      <a:da name="REF" value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/Mcl/MclConfig/dmaLogicChannel_Type"/>
                                    </v:ref>

                                    <v:ref name="UartDmaRxChannelRef" type="REFERENCE">
                                      <a:a name="DESC">
                                          <a:v>
                                            <![CDATA[EN:
                                             <html>
                                                <p>Reference to the DMA Rx channel, which is set in the Mcl driver configuration.</p>
                                                Note: Implementation Specific Parameter. <p/>
                                            </html>
                                            ]]>
                                          </a:v>
                                      </a:a>
                                      <a:a name="IMPLEMENTATIONCONFIGCLASS"
                                           type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                      </a:a>
                                      <a:a name="OPTIONAL" value="true"/>
                                      <a:a name="SCOPE" value="LOCAL"/>
                                      <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                      <a:a name="LABEL" value="Uart Rx DMA Channel"/>
                                      <a:a name="ORIGIN" value="NXP"/>
                                      <a:a name="UUID" value="ECUC:5419eaab-b78b-4ee2-9e88-22e201491b62"/>
                                      <a:a name="EDITABLE" type="XPath">
