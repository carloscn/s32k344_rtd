                                <a:da name="DEFAULT" value="false"/>
                            </v:var>


                                <v:var name="PlatformIpAPIsAvailable" type="BOOLEAN">
                                    <a:a name="LABEL" value="IP layer APIs"/>
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:
                                            <html>
                                                <p>PlatformIpAPIsAvailable</p>
                                                <p>Enable or disable IP layer APIs which are not used by APIs at High Level Driver (HLD). Following APIs are affected:</p>
                                                <li>IntCtrl_Ip_SetPending</li>
                                                <li>IntCtrl_Ip_GetPending</li>
                                                <li>IntCtrl_Ip_GetActive</li>
                                                <p></p>Note: Implementation Specific Parameter. <p/>
                                            </html>
                                        ]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="NXP"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="SCOPE" value="LOCAL"/>
                                    <a:a name="UUID" value="ECUC:72b8a638-4179-4639-124a-bb4daf844a8b"/>
                                    <a:da name="DEFAULT" value="false"/>
                                </v:var>
                                <!-- @implements PlatformEnableUserModeSupport_Object -->
                                <v:var name="PlatformEnableUserModeSupport" type="BOOLEAN">
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:<html>
                                            <p>When this parameter is enabled, the Platform module will adapt to run from User Mode, with the following measures:<p/>
                                            <p> b) using 'call trusted function' stubs for all internal function calls that access registers requiring supervisor mode.<p/>
                                            Note: Implementation Specific Parameter.
                                        </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS"
                                           type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="LABEL"
                                           value="Platform Enable User Mode Support"/>
                                    <a:a name="ORIGIN"
                                           value="NXP"/>
                                    <a:a name="SYMBOLICNAMEVALUE"
                                           value="false"/>
                                    <a:a name="UUID"
                                           value="ECUC:f466ec0d-5bba-65fa-be74-2bd1bb030b88"/>
                                    <a:da name="DEFAULT"
                                            value="false"/>
                                </v:var>
                                <!-- @implements PlatformMulticoreSupport_Object -->
                                <v:var name="PlatformMulticoreSupport" type="BOOLEAN">
                                    <a:a name="DESC">
                                    <a:v><![CDATA[EN:<html><p>This parameter globally enables the possibility to support multicore. If this parmeter is enabled, at least one EcucPartition needs to be defined (in all variants).<br>
                                    <h1>Note</h1>This is an Implementation Specific Parameter.
                                            </p></html>]]></a:v>
                                    </a:a>
                                    <a:a name="SCOPE" value="LOCAL"/>
                                    <a:a name="POSTBUILDVARIANTVALUE" value="false"/> 
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="LABEL" value="Platform Multicore Support"/>
                                    <a:a name="ORIGIN" value="NXP"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false" />
                                    <a:a name="UUID" value="ECUC:8436de9b-c7e1-4ab0-9e80-145e3870a2ab" />
                                    <a:da name="DEFAULT" value="false" />
                                    <a:a name="EDITABLE" type="XPath" expr="num:i(count(ecu:list('Platform.targetCoresList')))>1"/>
