                                        <a:tst expr="(. = 'true') and not(num:i(count(../../GeneralConfiguration/PlatformEcucPartitionRef/*) > 0))"
                                            true="Platform Multicore Support is only enabled when at least one EcucPartition needs to be defined (in all variants)."/>
                                            <a:tst expr="(. = 'true') and not(num:i(count(ecu:list('Platform.targetCoresList')))>1)"
                                            true="This derivative not support multicore so must be false "/>                            
                                    </a:a>
                                </v:var>

                                <v:var name="PlatformEnableVtorConfiguration" type="BOOLEAN">
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:<html>
                                            <p>When this parameter is enabled, the Platform module will allow the user the manually configure the Vector Table Offset Register address:<p/>
                                            Note: Implementation Specific Parameter.
                                        </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS"
                                           type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="LABEL"
                                           value="Platform Enable VTOR Manual Configuration"/>
