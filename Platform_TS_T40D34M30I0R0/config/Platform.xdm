                                    <a:a name="UUID"
                                           value="ECUC:73234c86-eecd-416f-ad21-91bd5a38ef27"/>
                                    <a:da name="DEFAULT"
                                            value="false"/>
                                    <a:da name="EDITABLE" 
                                            value="false"/>
                                    <a:da name="VISIBLE" 
                                            value="false"/>
                                </v:var>
                                <v:lst name="PlatformEcucPartitionRef">
                                    <a:da name="EDITABLE" type="XPath">
                                        <a:tst expr="../../GeneralConfiguration/PlatformMulticoreSupport"/>
                                    </a:da>
                                    <v:ref name="PlatformEcucPartitionRef" type="REFERENCE">
                                        <a:a name="DESC">
                                            <a:v>
                                                <![CDATA[EN:
                                                <html>
                                                <p>Maps the Platform driver to zero a multiple ECUC partitions to make the modules API available in this partition.</p>
                                                <p>Note: Each PlatformEcucPartitionRef should map to a M7 core, one by one</p>
                                                </html>
                                                ]]>
                                            </a:v>
                                        </a:a>
                                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                            type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PreCompile">VariantPostBuild</icc:v>
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                        </a:a>
                                        <a:a name="ORIGIN" value="NXP"/>
                                        <a:a name="SCOPE" value="ECU"/>
                                        <a:a name="UUID" 
                                            value="ECUC:f86d720b-a1e5-4f3f-a656-0f491v2l8563"/>
                                        <a:da name="REF" value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/EcuC/EcucPartitionCollection/EcucPartition"/>
                                        <a:da name="INVALID" type="XPath">
                                            <a:tst expr="text:uniq(../*, .)" false="Duplicate Partition."/>
                                            <a:tst expr="count(text:grep(node:refs('ASPathDataOfSchema:/TS_T40D34M30I0R0/Os')/OsApplication/*/OsAppEcucPartitionRef, .)) = 0"
                                            true="This ECUC partition must be defined in OS."/>
                                            <a:tst expr="count(./../../../McmConfig/*[./PlatformMcmEcucPartitionRef = node:current()]) = 0 and count(./../../../IntCtrlConfig/*[./PlatformNvicEcucPartitionRef = node:current()]) = 0 and (./../../../IntmConfig/IntmGenericSettings/PlatformIntmGenericEcucPartitionRef = node:current()) = 0 and count(./../../../IntmConfig/IntmChannel/*[./PlatformIntmChannelEcucPartitionRef = node:current()]) = 0 and count(./../../../MPU_M7_Configuration/MPU_M7_ModuleConfig/*[./Mpu_M7_EcucPartitionRef = node:current()]) = 0 and count(./../../../Mpu_M33_Configuration/Mpu_M33_ModuleConfig/*[./Mpu_M33_EcucPartitionRef = node:current()]) = 0 and
                                            count(./../../../MPU_R52_Configuration/*[./MPU_R52_EcucPartitionRef = node:current()]) = 0"
                                            true="This PlatformEcucPartitionRef is not used anywhere in Platform"/>
                                        </a:da>
                                    </v:ref>
                                </v:lst>
                            </v:ctr>



