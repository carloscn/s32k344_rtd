                            <!-- @implements LinFrameTimeoutDisable_Object -->
                            <v:var name="LinFrameTimeoutDisable" type="BOOLEAN">
                                <a:a name="LABEL" value="Lin Frame Timeout Disable"/>
                                <a:a name="DESC">
