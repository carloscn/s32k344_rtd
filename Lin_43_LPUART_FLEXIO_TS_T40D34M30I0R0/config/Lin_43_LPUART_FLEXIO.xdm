                                            <a:a name="OPTIONAL" value="true"/>
                                            <a:da name="REF" value="ASPathDataOfSchema:/TS_T40D34M30I0R0/Mcl/MclConfig/FlexioCommon/FlexioMclLogicChannels"/>
                                            <a:da name="INVALID" type="XPath">

                                            <a:tst  expr="text:uniq(node:fallback( ../../*/LinFlexioTxControllerRef, 0 ), node:fallback( ., 1 ))"
                                                    false="Flexio Lin tx channel used must unique across all FLEXIO channel referenced."/>
                                            <a:tst expr="contains(../LinHwChannel, 'LPUART') or node:value(.) != node:value(../LinFlexioRxControllerRef)" false="Flexio Lin Tx channel used must unique across all FLEXIO channel referenced."/>
                                            <a:tst expr="contains(../LinHwChannel, 'LPUART') or node:refvalid(.)" false="Invalid or empty reference."/>
