                                        <a:a name="UUID" value="ECUC:59f8b925-adff-4508-8c3e-219d335e9f24"/>
                                        <a:da name="REF" value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/EcuM/EcuMConfiguration/EcuMCommonConfiguration/EcuMWakeupSource"/>
                                        <a:da name="INVALID" type="XPath">
                                             <a:tst expr="( (. != '') and (node:exists(node:refs(.))) )" false="Refer to a invalid wakeup source"/>
                                             <a:tst expr="node:fallback(../LinChannelWakeupSupport, 'false')= 'false'"
                                             true="The LinChannelEcuMWakeupSource node should be disabled because the LinChannelWakeupSupport node was disabled"/>
                                        </a:da>
                                    </v:ref>
                                    <!-- @implements LinChannelEcucPartitionRef_Object -->
                                    <v:ref name="LinChannelEcucPartitionRef" type="REFERENCE">
                                        <a:a name="DESC">
                                            <a:v>
                                                <![CDATA[EN:<html>
                                                    Maps one single Lin channel to zero or one ECUC partitions.
                                                    The ECUC partition referenced is a subset of the ECUC partitions where the Lin driver is mapped to.
                                                </html>]]>
                                            </a:v>
                                        </a:a>
                                        <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                          <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                          <icc:v class="PreCompile">VariantPostBuild</icc:v>
                                        </a:a>
                                        <a:a name="OPTIONAL" value="true"/>
                                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                        <a:a name="SCOPE" value="ECU"/>
                                        <a:a name="UUID" value="ECUC:b016c50a-d2c6-4fe5-b096-75a9bfac79ff"/>
                                        <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                        <a:a name="POSTBUILDVARIANTMULTIPLICITY" value="true"/>
                                        <a:da name="REF" value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/EcuC/EcucPartitionCollection/EcucPartition"/>
                                        <a:da name="INVALID" type="XPath">
                                            <a:tst expr="node:refvalid(.)" false="Invalid or empty reference."/>
                                            <a:tst expr="node:containsValue(../../../../LinGeneral/LinEcucPartitionRef/*, node:value(.))"
                                                false="The ECUC partitions referenced by LinChannelEcucPartitionRef shall be a subset of the ECUC partitions referenced by LinEcucPartitionRef."/>
                                        </a:da>
                                    </v:ref>

