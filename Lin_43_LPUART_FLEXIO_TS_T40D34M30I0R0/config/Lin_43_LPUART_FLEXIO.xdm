                        <!-- @implements LinGeneral_Object -->
                        <v:ctr name="LinGeneral" type="IDENTIFIABLE">
                            <a:a name="DESC">
                                <a:v>
                                    <![CDATA[EN:
                                        <html>
                                            <h1>LinGeneral</h1>
                                            <p>Autosar Requirements: ECUC_Lin_00183</p>
                                            <p>This container contains the parameters related to each LIN Driver Unit.
                                               This container is a MultipleConfigurationContainer, i.e. this container and
                                               its sub-containers exit once per configuration set.</p>
                                        </html>
                                    ]]>
                                </a:v>
                            </a:a>
                            <a:a name="UUID" value="ECUC:386c52b6-2650-435a-a896-b6f81bccf83a"/>
							<!-- /* @implements LinMulticoreSupport_Object */ -->
                            <v:var name="LinMulticoreSupport" type="BOOLEAN">
                                <a:a name="LABEL" value="Lin Multicore Support"/>
                                <a:a name="DESC">
                                    <a:v>
                                        <![CDATA[EN:
                                            <html>
                                                <p>This parameter determine multi-core feature will be used in Lin driver.</p>
                                                <p>If LinMulticoreSupport is disabled, then for all the variants no partition shall be defined.</p>
                                                <p>If LinMulticoreSupport is enabled, at least one EcucPartition needs to be defined (in all variants).</p>
                                            </html>
                                        ]]>
                                    </a:v>
                                </a:a>
                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                    <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                </a:a>
                                <a:a name="ORIGIN" value="NXP"/>
                                <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                                <a:a name="SCOPE" value="LOCAL"/>
                                <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                <a:a name="UUID" value="ECUC:72a8a638-4009-4639-124a-ee4da2363e77"/>
                                <a:da name="DEFAULT" value="false"/>
                                <a:a name="EDITABLE" type="XPath">
                                    <a:tst expr="(ecu:get(&apos;Lin.LinMulticoreSupport&apos;)='TRUE') or
                                        ((ecu:get(&apos;Lin.LinMulticoreSupport&apos;)='FALSE') and (.='true'))"/>
                                </a:a>
                                <a:da name="INVALID" type="XPath">
                                  <a:tst expr="(ecu:get(&apos;Lin.LinMulticoreSupport&apos;)='FALSE') and (.='true')"
                                    true="This derivative is not supported multicore. Please uncheck this node."/>
                                </a:da>
                            </v:var>
                            <!-- @implements LinDevErrorDetect_Object -->
                            <v:var name="LinDevErrorDetect" type="BOOLEAN">
                                <a:a name="LABEL" value="Lin Default Error Detection"/>
                                <a:a name="DESC">
                                    <a:v>
                                        <![CDATA[EN:
                                            <html>
                                                <h1>LinDevErrorDetect</h1>
                                                <p>Autosar Requirements: ECUC_Lin_00066</p>
                                                <p>Switches the Default Error Detection and Notification ON or OFF.</p>
                                            </html>
                                        ]]>
                                    </a:v>
                                </a:a>
                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                    <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                </a:a>
                                <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                                <a:a name="SCOPE" value="LOCAL"/>
                                <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                <a:a name="UUID" value="ECUC:7228a638-4009-4639-8e1a-bb3d9f762a7b"/>
                                <a:da name="DEFAULT" value="false"/>
                            </v:var>
                            <!-- @implements LinIndex_Object -->
                            <v:var name="LinIndex" type="INTEGER">
                                <a:a name="LABEL" value="InstanceId"/>
                                <a:a name="DESC">
                                    <a:v>
                                        <![CDATA[EN:
                                            <html>
                                                <p>Autosar Requirements: ECUC_Lin_00179</p>
                                                <p>Specifies the InstanceId of this module instance. If only one instance is present it shall have the Id 0.</p>
                                                <p>Note, this parameter is not used in the current implementation.</p>
                                            </html>
                                        ]]>
                                    </a:v>
                                </a:a>
                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                    <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                </a:a>
                                <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                                <a:a name="SCOPE" value="LOCAL"/>
                                <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                <a:a name="UUID" value="ECUC:8a1935ff-76b3-4a7e-aa67-e516f2704cfa"/>
                                <a:da name="DEFAULT" value="0"/>
                                <a:da name="READONLY" value="true"/>
                                <a:da name="INVALID" type="Range">
                                    <a:tst expr="&lt;=255"/>
                                    <a:tst expr="&gt;=0"/>
                                </a:da>
                            </v:var>

                            <v:var name="LinTimeoutMethod" type="ENUMERATION">
                                <a:a name="LABEL" value="Lin Timeout Method"/>
                                <a:a name="DESC">
                                    <a:v>
                                        <![CDATA[EN:
                                            <html>
                                                <p>LinTimeoutMethod</p>
                                                <p>Configures the timeout method.</p>
                                                <p>Based on this selection a certain timeout method from OsIf will be used in the driver.</p>
                                                <p>This parameter is used to select between different OsIf counter implementations. For additional details, please refer to the OsIf documentation.</p>
                                                <p>Note: If SystemTimer or CustomTimer are selected make sure the corresponding timer is enabled in OsIf General configuration.</p>
                                                Note: Implementation Specific Parameter. <p/>
                                            </html>
                                        ]]>
                                    </a:v>
                                </a:a>
                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                    <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                </a:a>
                                <a:a name="ORIGIN" value="NXP"/>
                                <a:a name="SCOPE" value="LOCAL"/>
                                <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                <a:a name="UUID" value="ECUC:3b8b947e-d4c1-425a-b785-a7d121c7255f"/>
                                <a:da name="INVALID" type="XPath">
                                    <a:tst expr="node:refs('ASPathDataOfSchema:/TS_T40D34M30I0R0/BaseNXP/OsIfGeneral/OsIfUseCustomTimer') = 'false' and node:fallback(.,'OSIF_COUNTER_DUMMY') = 'OSIF_COUNTER_CUSTOM'" true="Custom Timer is not enabled in OsIf (OsIfGeneral/OsIfUseCustomTimer checkbox)"/>
                                    <a:tst expr="node:refs('ASPathDataOfSchema:/TS_T40D34M30I0R0/BaseNXP/OsIfGeneral/OsIfUseSystemTimer') = 'false' and node:fallback(.,'OSIF_COUNTER_DUMMY') = 'OSIF_COUNTER_SYSTEM'" true="System Timer is not enabled in OsIf (OsIfGeneral/OsIfUseSystemTimer checkbox)"/>
                                </a:da>
                                <a:da name="RANGE">
                                    <a:v>OSIF_COUNTER_DUMMY</a:v>
                                    <a:v>OSIF_COUNTER_SYSTEM</a:v>
                                    <a:v>OSIF_COUNTER_CUSTOM</a:v>
                                </a:da>
                                <a:a name="DEFAULT" value="OSIF_COUNTER_DUMMY"/>
                            </v:var>

                            <!-- @implements LinTimeoutDuration_Object -->
                            <v:var name="LinTimeoutDuration" type="INTEGER">
                                <a:a name="LABEL" value="Lin Timeout Duration"/>
                                <a:a name="DESC">
                                    <a:v>
                                        <![CDATA[EN:
                                            <html>
                                                Specifies the maximum number of loops for blocking function until a timeout is raised in short term wait loops. If LinTimeoutMethod is OSIF_COUNTER_SYSTEM or OSIF_COUNTER_CUSTOM, LinTimeoutDuration is in microsecond value. If LinTimeoutMethod is OSIF_COUNTER_DUMMY, the LinTimeoutDuration  is number of wait loop.
                                            </html>
                                        ]]>
                                    </a:v>
                                </a:a>
                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                    <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                </a:a>
                                <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                                <a:a name="SCOPE" value="LOCAL"/>
                                <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                <a:a name="UUID" value="ECUC:ea0e01de-6a8a-4565-840a-76cb28e5c197"/>
                                <a:da name="DEFAULT" value="1000"/>
                                <a:da name="INVALID" type="Range">
                                    <a:tst expr="&lt;=65535"/>
                                    <a:tst expr="&gt;=0"/>
                                </a:da>
                            </v:var>


                            <!-- @implements LinVersionInfoApi_Object -->
                            <v:var name="LinVersionInfoApi" type="BOOLEAN">
                                <a:a name="LABEL" value="Provide Lin VersionInfo Api"/>
                                <a:a name="DESC">
                                    <a:v>
                                       <![CDATA[EN:
                                            <html>
                                                <h1>LinVersionInfoApi</h1>
                                                <p>Autosar Requirements: ECUC_Lin_00067</p>
                                                <p>Switches the Lin_GetVersionInfo function ON or OFF.</p>
                                            </html>
                                       ]]>
                                    </a:v>
                                </a:a>
                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                    <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                </a:a>
                                <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                                <a:a name="SCOPE" value="LOCAL"/>
                                <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                <a:a name="UUID" value="ECUC:be230747-6176-4c0b-90e2-290362e2d3ef"/>
                                <a:da name="DEFAULT" value="false"/>
                            </v:var>
                            <!-- /** @implements LinEcucPartitionRef_Object */ -->
                            <v:lst name="LinEcucPartitionRef">
                                <a:da name="INVALID" type="XPath">
                                    <a:tst expr="( (node:fallback(as:modconf('Lin')[1]/LinGeneral/LinMulticoreSupport, 'false') = 'false') and (num:i(count(./*)) != 0) )"
                                    true="Multicore isn't supported(LinMulticoreSupport node is unchecked) so the LinEcucPartitionRef list need to be empty"/>
                                    <a:tst expr="( (node:fallback(as:modconf('Lin')[1]/LinGeneral/LinMulticoreSupport, 'false') = 'true') and (num:i(count(./*)) = 0) )"
                                    true="Multicore is supported(LinMulticoreSupport node is checked) so at least one partition should be referred"/>
                                </a:da>
                                <v:ref name="LinEcucPartitionRef" type="REFERENCE">
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:<html>
                                                Maps the Lin driver to zero or multiple ECUC partitions to make the modules API available in this partition.
                                                The Lin driver will operate as an independent instance in each of the partitions.
                                            </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                      <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                      <icc:v class="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                    <a:a name="SCOPE" value="ECU"/>
                                    <a:a name="UUID" value="ECUC:b016dd0a-d2c6-4fe5-b096-75f6bfac79ff"/>
                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                    <a:a name="POSTBUILDVARIANTMULTIPLICITY" value="true"/>
                                    <a:da name="REF" value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/EcuC/EcucPartitionCollection/EcucPartition"/>
                                    <a:da name="INVALID" type="XPath">
                                        <a:tst expr="node:refvalid(.)" false="Invalid or empty reference."/>
                                        <a:tst expr="node:containsValue(as:modconf('Os')[1]/OsApplication/*/OsAppEcucPartitionRef, node:value(.))"
                                        false="The partition referred isn't used by any Os/OsApplication/*/OsAppEcucPartitionRef"/>
                                        <a:tst expr="text:uniq(node:fallback(../*,''),node:fallback( ., '' ))"
                                        false ="The partition must be unique in LinEcucPartitionRef list."/>
                                        <a:tst expr="node:containsValue(as:modconf('Lin')/LinGlobalConfig/LinChannel/*/LinChannelEcucPartitionRef, node:value(.))"
                                        false ="The partition referred in LinEcucPartitionRef is not used by any LinChannelEcucPartitionRef."/>
                                    </a:da>
                                </v:ref>
                            </v:lst>

                        </v:ctr>
                        <v:ctr name="AutosarExt" type="IDENTIFIABLE">
                            <a:a name="UUID" value="ECUC:fda4a87f-3328-4f1c-a94a-15f9bc8b33ca"/>
                            <a:a name="DESC">
                                <a:v>
                                    <![CDATA[EN:
                                        <html>
                                            <h1>AutosarExt</h1>
                                            <p>Autosar Requirements: </p>
                                            <p>This container contains the global configuration parameters of the Non-Autosar Lin driver.
                                               This container is a MultipleConfigurationContainer, i.e. this container and
                                               its sub-containers exist once per configuration set.</p>
                                        </html>
                                    ]]>
                                </a:v>
                            </a:a>
                            <v:var name="LinDisableDemReportErrorStatus" type="BOOLEAN">
                                <a:a name="LABEL" value="Lin Disable Production Error Reporting"/>
                                <a:a name="DESC">
