                                            <a:tst expr="contains(../LinHwChannel, 'LPUART') or node:value(.) != node:value(../LinFlexioTxControllerRef)" false="Flexio Lin Rx channel used must be different than the Flexio Lin Tx channel."/>
                                            <a:tst expr="contains(../LinHwChannel, 'LPUART') or node:refvalid(.)" false="Invalid or empty reference."/>
