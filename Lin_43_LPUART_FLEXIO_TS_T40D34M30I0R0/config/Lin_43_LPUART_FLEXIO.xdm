                                            <a:tst expr="(not(node:exists(../LinFlexioRxControllerRef) and node:exists(../LinFlexioTxControllerRef)) and contains(., 'FLEXIO'))" true="Lin Flexio Rx Channel Reference and Lin Flexio Tx Channel Reference should be configured"/>
                                        </a:da>
                                        <a:da name="RANGE" type="XPath" expr="ecu:list(&apos;Lin.LinGlobalConfig.LinChannel.LinHwChannel&apos;)"/>
                                    </v:var>
