                            <!-- @implements OcuGeneral_Object -->
                            <v:ctr name="OcuGeneral" type="IDENTIFIABLE">
                                <a:a name="UUID" value="ECUC:fabfd77f-4f07-4ffb-a24a-c8ec54951d22"/>
                                <a:a name="DESC">
                                    <a:v>
                                    <![CDATA[EN:
                                    <html>
                                    This container contains the module-wide configuration parameters of the OCU Driver.
                                    </html> ]]>
                                    </a:v>
                                </a:a>         


                                <v:var name="OcuMulticoreEnabled" type="BOOLEAN">
                                    <a:a name="DESC">
                                        <a:v><![CDATA[EN:
                                            <html>
                                                <p>Switch to enable/disable multi-core feature.</p>
                                                <p>
                                                    User can choose ENABLE multi-core feature by checking this option, this will force to configure at least 1 ECUC partition in 
                                                    <b>OcuChannelEcucPartitionRef</b>, and each OCU channel in <b>OcuChannel</b> to configure at least 1 ECUC partition reference
                                                    in <b>OcuChannelEcucPartitionRef</b> container to fulfill generating code condition; OR unchecked this option to DISABLE 
                                                    multi-core feature, performing this action will force user to remove all ECUC partition reference in every OCU channels contained
                                                    in <b>OcuChannel</b> and in <b>OcuChannelEcucPartitionRef</b>.
                                                </p>
                                            </html> ]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="NXP"/>
                                    <a:a name="SCOPE" value="LOCAL"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:2e5f2e8d-3657-4d05-81f1-2bd1bb030b88"/>
                                    <a:da name="DEFAULT" value="false"/>
                                    <a:da name="INVALID" type="XPath">
                                    <a:tst expr="node:value(.) = 'false' and num:i(count(../OcuEcucPartitionRef/*)) != 0"
                                    true="If the number OcuEcucPartitionRef != 0, The check-box OcuMulticoreEnabled must be 'true'"/>
                                    <a:tst expr="node:value(.) = 'true' and num:i(count(../OcuEcucPartitionRef/*)) = 0"
                                    true="If the number OcuEcucPartitionRef = 0, The check-box OcuMulticoreEnabled must be 'false'"/>
                                    <a:tst expr="node:value(.) = 'true' and ecu:get('Ocu.MulticoreSupport') = 'OFF'"
                                    true="This target don't support multi core, The check-box OcuMulticoreEnabled must be 'false' "/>
                                    </a:da> 
                                </v:var>

                                <!-- @implements OcuDevErrorDetect_Object -->
                                <v:var name="OcuDevErrorDetect" type="BOOLEAN">
                                    <a:a name="DESC">
