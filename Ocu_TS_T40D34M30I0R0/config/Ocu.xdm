                                            <a:tst expr="node:exists(../../../OcuConfigSet/OcuChannel/*/OcuChannelEcucPartitionRef/*[node:value(.) = node:current()])" false="This partition not use any ocu channels"/>
                                        </a:da>
                                    </v:ref>
                                </v:lst>

                                <v:ref name="OcuKernelEcucPartitionRef" type="REFERENCE">
                                    <a:a name="DESC">
                                        <a:v><![CDATA[EN:
                                            <html>
                                                Maps the OCU kernel to zero or one ECUC partitions to assign the driver kernel to a certain core.
                                                The ECUC partition referenced is a subset of the ECUC partitions where the OCU driver is mapped to.
                                            </html> ]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
