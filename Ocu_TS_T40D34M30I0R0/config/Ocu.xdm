                            <!-- @implements OcuConfigSet_Object -->
                            <v:ctr name="OcuConfigSet" type="IDENTIFIABLE">
                                <a:a name="DESC">
                                    <a:v><![CDATA[EN:
                                    <html>
                                         This container is the base of a Configuration Set, which contains the configured OCU channels. This way, different configuration sets can be defined for post-build process.
                                    </html> ]]>
                                    </a:v>
                                </a:a>
                                <a:a name="UUID" value="ECUC:5ce51b69-fffb-4b87-863d-7463e09eeeb1"/>
                            
                                <!-- @implements OcuCountdirection_Object -->
                                <v:var name="OcuCountdirection" type="ENUMERATION">
                                    <a:a name="DESC">
                                        <a:v><![CDATA[EN:
                                         <html>
                                         This parameter indicates the count direction for the whole OCU driver.
                                         <br><h1>Note</h1>eMios IP does not support down counting. Up-counting is always used as default</br>
                                         </html> ]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                    <a:a name="POSTBUILDVARIANTMULTIPLICITY" value="true"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                    </a:a>
                                    <a:a name="OPTIONAL" value="true"/>
                                    <a:a name="READONLY" value="true"/>
                                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                    <a:a name="SCOPE" value="LOCAL"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:a8dc410c-633d-410d-ba43-d59bf7b86a6e"/>
                                    <a:da name="DEFAULT" value="OCU_UPCOUNTING"/>
                                    <a:da name="RANGE">
                                        <a:v>OCU_DOWNCOUNTING</a:v>
                                        <a:v>OCU_UPCOUNTING</a:v>
                                    </a:da>
                                </v:var>
                     
                                <v:lst name="OcuChannel" type="MAP">
                                    <a:da name="MIN" value="1"/>
                                    <a:a name="MAX" type="XPath" expr="num:i(count(ecu:list('Ocu.Hw_Element_List')))"/>
                              
                                    <!-- @implements OcuChannel_Object -->
                                    <v:ctr name="OcuChannel" type="IDENTIFIABLE">
                                        <a:a name="DESC">
                                            <a:v><![CDATA[EN:
                                            <html>
                                            Configuration of an individual OCU channel.
                                            </html> ]]>
                                            </a:v>
                                        </a:a>
                                        <a:a name="REQUIRES-INDEX" value="true"/>                                        
                                        <a:a name="UUID" value="ECUC:8d95a4f1-9d41-4ab0-80fb-341e78968c4b"/>
                                        <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                            <icc:v mclass="PreCompile">VariantPostBuild</icc:v>
                                            <icc:v mclass="PreCompile">VariantPreCompile</icc:v>
                                        </a:a>
                                         
                                        <!-- @implements OcuChannelId_Object -->
                                        <v:var name="OcuChannelId" type="INTEGER">
                                            <a:a name="DESC">
                                                <a:v><![CDATA[EN:
                                                <html>
                                                Channel Id of the OCU channel. This value will be assigned to the symbolic name derived from the OcuChannel container short name. It defines the assignment of the channel to the physical OCU hardware channel.
                                                </html> ]]>
                                                </a:v>
                                            </a:a>
                                            <a:a name="IMPLEMENTATIONCONFIGCLASS"  type="IMPLEMENTATIONCONFIGCLASS">
                                                <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                                <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                            </a:a>
                                            <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                            <a:a name="SCOPE" value="LOCAL"/>
                                            <a:a name="SYMBOLICNAMEVALUE" value="true"/>
                                            <a:a name="UUID" value="ECUC:ae7f2b9b-74ab-4a31-9d76-7c4eca6f36ac"/>
                                            <a:da name="INVALID" type="Range">
                                                <a:tst expr="&lt;=65535"/>
                                                <a:tst expr="&gt;=0"/>
                                            </a:da>
                                            <a:da name="DEFAULT" type="XPath" expr="node:fallback(node:current()/../@index, num:i(1))"/>
                                            <a:da name="INVALID" type="XPath">
                                                <a:tst expr="(node:value(.) >= num:i(count(../../*/.)))" 
                                                true="Ocu channel IDs is out of range maximum value is given by the total number of channels."/>
                                                <a:tst expr="text:uniq(../../*/OcuChannelId, .)"
                                                false="Ocu channels should have unique IDs."/>
                                            </a:da>
                                        </v:var> 
                                        
                                        <!-- @implements OcuAssignedHardwareChannel_Object -->
                                        <v:var name="OcuAssignedHardwareChannel" type="INTEGER">
                                            <a:a name="DESC">
                                                <a:v><![CDATA[EN:
                                                <html>
                                                The physical hardware channel that is assigned to this logical channel.
                                                <br><h1>Note</h1> Please use OcuHWSpecificSettingsRef parameter to assign a hardware channel. <br>
                                                </html> ]]>
                                                </a:v>
                                            </a:a>
                                            <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                            </a:a>
                                            <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                            <a:a name="SCOPE" value="LOCAL"/>
                                            <a:a name="READONLY" value="true"/>
                                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                            <a:a name="UUID" value="ECUC:41b79bdf-db86-4a56-b6f6-ce1a3dcd3491"/>
                                            <a:da name="DEFAULT" type="XPath" expr="node:fallback(node:current()/../@index, num:i(1))"/>
                                            <a:da name="INVALID" type="Range">
                                                <a:tst expr="&lt;=255"/>
                                                <a:tst expr="&gt;=0"/>
                                            </a:da>
                                        </v:var>
                                        
                                        <!-- @implements OcuHWSpecificSettingsRef_Object -->
                                        <v:ref name="OcuHWSpecificSettingsRef" type="REFERENCE">
                                            <a:a name="DESC">
                                            <a:v><![CDATA[EN:
                                                <html>
                                                Reference to the OcuHWSpecificSettings used by the OcuChannel.
                                                </html>
                                                ]]></a:v>
                                            </a:a>
                                            <a:a name="IMPLEMENTATIONCONFIGCLASS"
                                                 type="IMPLEMENTATIONCONFIGCLASS">
                                                <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                                <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                            </a:a>
                                            <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                            <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                            <a:a name="UUID"
                                                 value="ECUC:41b79bdf-dc86-4a56-b6f6-ce1a3dcd3491"/>
                                            <a:da name="REF">
                                                <a:v>ASPathDataOfSchema:/AUTOSAR/EcucDefs/Ocu/OcuConfigSet/OcuHWSpecificSettings</a:v>
                                            </a:da>
                                            <a:da name="INVALID" type="XPath">
                                                <a:tst expr="(string(node:value(.)) = '')"
                                                    true="Please select a reference to a configured hardware channel"/>
                                                <a:tst expr="not(text:uniq(../../*/OcuHWSpecificSettingsRef, .)) and (string(node:value(.)) != '')"
                                                    true="Duplicate physical channel"/>
                                                <a:tst expr="node:exists(node:current()/../OcuChannelEcucPartitionRef/*) and node:exists(./../../*[substring-before(node:value(node:ref(./OcuHWSpecificSettingsRef)/OcuHardwareElements),'_CH') = substring-before(node:value(node:ref(node:current())/OcuHardwareElements),'_CH')]/OcuChannelEcucPartitionRef/*[node:value(.) != node:value(node:current()/../OcuChannelEcucPartitionRef/*)])" 
                                                    true="Please select the same OcuEcucPartitionRef for all configured channels within this Emios instance"/>
                                            </a:da>
                                        </v:ref>

                                        <!-- @implements OcuChannelEcucPartitionRef_Object -->
                                        <v:lst name="OcuChannelEcucPartitionRef">
                                        <a:da name="INVALID" type="XPath">
                                        <a:tst expr="0 = node:exists(./*[1]) and 1 = node:exists(../../../../OcuGeneral/OcuEcucPartitionRef/*[1])" true="please add a Partition in list OcuEcucPartitionRef "/>
                                        </a:da>
                                            <v:ref name="OcuChannelEcucPartitionRef" type="REFERENCE">
                                                <a:a name="DESC">
                                                    <a:v><![CDATA[EN:
                                                        <html>
                                                            Maps a OCU channel to zero or multiple ECUC partitions to limit the access to this channel group.
                                                            The ECUC partitions referenced are a subset of the ECUC partitions where the OCU driver is mapped to.
                                                        </html> ]]>
                                                    </a:v>
                                                </a:a>
                                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                    <icc:v mclass="PreCompile">VariantPostBuild</icc:v>
                                                    <icc:v mclass="PreCompile">VariantPreCompile</icc:v>
                                                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                                    <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                </a:a>
                                                <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                                <a:a name="POSTBUILDVARIANTMULTIPLICITY" value="true"/>
                                                <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                <a:a name="SCOPE" value="ECU"/>
                                                <a:a name="UUID" value="ECUC:14ddde73-619e-4029-a793-3535990395ad"/>
                                                <a:da name="REF" value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/EcuC/EcucPartitionCollection/EcucPartition"/>
                                                <a:da name="INVALID" type="XPath">
                                                    <a:tst expr="node:exists(../../../../../OcuGeneral/OcuEcucPartitionRef/*[node:value(.) = node:current()])" false="please select a Partition in list OcuEcucPartitionRef "/>
                                                    <a:tst expr="@index > 0" true="Ocu does not support Numbers Partition >> 1"/>
                                                </a:da>
                                            </v:ref>
                                        </v:lst>

                                        <!-- @implements OcuDefaultThreshold_Object -->
                                        <v:var name="OcuDefaultThreshold" type="INTEGER">
                                            <a:a name="DESC">
                                                <a:v><![CDATA[EN:
                                                <html>
                                                Value of comparison threshold used for Initialization.
                                                </html> ]]>
                                                </a:v>
                                            </a:a>
